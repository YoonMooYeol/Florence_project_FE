# 구글 캘린더 API 키
VUE_APP_GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY

# 구글 캘린더 ID
VUE_APP_GOOGLE_CALENDAR_ID=primary

# 환경 설정 (배포 환경)
VITE_APP_ENV=production

# 소셜 로그인 설정
VITE_KAKAO_REST_API_KEY=YOUR_KAKAO_REST_API_KEY
VITE_KAKAO_REDIRECT_URI=https://your-backend-domain.com/v1/accounts/kakao/callback

# 네이버 로그인 설정
# 주의: 네이버 개발자 센터에 등록된 Callback URL과 정확히 일치해야 함
VITE_NAVER_CLIENT_ID=YOUR_NAVER_CLIENT_ID
# 백엔드 URL을 콜백으로 사용 (네이버 개발자 센터에 이 URL이 등록되어 있어야 함)
VITE_NAVER_REDIRECT_URI=https://your-backend-domain.com/v1/accounts/naver/callback

# 구글 로그인 설정
VITE_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID
VITE_GOOGLE_REDIRECT_URI=https://your-backend-domain.com/v1/accounts/google/callback

# API 서버 주소
VITE_API_BASE_URL=https://your-backend-domain.com/v1 